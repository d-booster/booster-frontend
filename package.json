{
  "name": "booster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:eslint": "next lint",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "typecheck": "tsc --noEmit",
    "lint": "npm-run-all lint:eslint typecheck",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "pre-commit": "npx lint-staged --allow-empty",
    "pre-push": "git diff --quiet --exit-code"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "lint-staged": "15.2.7",
    "npm-run-all": "4.1.5",
    "postcss": "^8",
    "prettier": "3.3.3",
    "simple-git-hooks": "2.11.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run pre-commit",
    "pre-push": "npm run pre-push"
  },
  "volta": {
    "node": "20.0.0"
  }
}
